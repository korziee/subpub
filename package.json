{
  "name": "subpub",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "tsc -b",
    "start": "docker compose up -d",
    "stop": "docker compose stop",
    "logs": "docker compose logs -f",
    "repl": "docker-compose run client yarn start:client",
    "start:client": "yarn build && node -i -e \"$(< ./dist/services/client/index.js)\"",
    "start:router": "nodemon ./src/services/router/index.ts",
    "start:sub-node": "nodemon ./src/services/sub-node/index.ts",
    "client": "nodemon ./src/services/client/index.ts",
    "test": "jest"
  },
  "dependencies": {
    "@trpc/client": "^10.0.0-rc.7",
    "@trpc/server": "^10.0.0-rc.7",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@types/jest": "^29.2.2",
    "@types/node": "^18.11.9",
    "jest": "^29.3.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  }
}
